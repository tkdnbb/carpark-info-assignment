{
    "openapi": "3.0.0",
    "info": {
      "title": "Carpark API",
      "version": "1.0.0",
      "description": "API for filtering carparks and adding favorites"
    },
    "servers": [
      {
        "url": "http://localhost:4000",
        "description": "Local server"
      }
    ],
    "paths": {
      "/carparks": {
        "get": {
          "summary": "Filter Carparks",
          "description": "Retrieves a list of carparks based on the provided filters. All parameters are optional",
          "parameters": [
            {
              "name": "free_parking",
              "in": "query",
              "description": "Filter carparks that offer free parking",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["YES", "NO"]
              }
            },
            {
              "name": "night_parking",
              "in": "query",
              "description": "Filter carparks that offer night parking",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["YES", "NO"]
              }
            },
            {
              "name": "gantry_height",
              "in": "query",
              "description": "Filter carparks that meet the specified gantry height",
              "required": false,
              "schema": {
                "type": "number",
                "format": "float"
              }
            },
            {
              "name": "limit",
              "in": "query",
              "description": "Limit the number of carparks returned",
              "required": false,
              "schema": {
                "type": "integer",
                "format": "int32"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "A list of carparks",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Carpark"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/carparks/add_favorite": {
        "post": {
          "summary": "Add Favorite",
          "description": "Adds a carpark to the user's favorites.",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AddFavoriteRequest"
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successfully added to favorites",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/AddFavoriteResponse"
                  }
                }
              }
            }
          }
        }
      },
      "/carparks/favorites": {
        "post": {
          "summary": "Get Favorites",
          "description": "Retrieves the list of favorite carparks for a user.",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FavoritesRequest"
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "A list of favorite carparks",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Carpark"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "Carpark": {
          "type": "object",
          "properties": {
            "car_park_no": {
              "type": "string"
            },
            "address": {
              "type": "string"
            },
            "x_coord": {
              "type": "number",
              "format": "float"
            },
            "y_coord": {
              "type": "number",
              "format": "float"
            },
            "car_park_type": {
              "type": "string"
            },
            "type_of_parking_system": {
              "type": "string"
            },
            "short_term_parking": {
              "type": "string"
            },
            "free_parking": {
              "type": "string"
            },
            "night_parking": {
              "type": "string"
            },
            "car_park_decks": {
              "type": "integer"
            },
            "gantry_height": {
              "type": "number",
              "format": "float"
            },
            "car_park_basement": {
              "type": "string"
            }
          }
        },
        "FavoritesRequest": {
          "type": "object",
          "properties": {
            "user_id": {
              "type": "string"
            }
          },
          "required": ["user_id"]
        },
        "AddFavoriteRequest": {
          "type": "object",
          "properties": {
            "user_id": {
              "type": "string"
            },
            "car_park_no": {
              "type": "string"
            }
          },
          "required": ["user_id", "car_park_no"]
        },
        "AddFavoriteResponse": {
          "type": "object",
          "properties": {
            "message": {
              "type": "string"
            }
          }
        }
      }
    }
  }
  